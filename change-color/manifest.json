{
    "name": "change-color",
    "version": "0.1.0",
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_title": "Change Color",
      "default_popup":"popup.html",
    "default_icon": "colorLogo.png"
      },
      "icons": {
        "128": "colorLogo.png"
      }
  }